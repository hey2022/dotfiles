<?xml version="1.0"?>
<QALCULATE version="5.8.1">
  <function>
    <names>v2p</names>
    <expression>[magnitude(\x), atan2(element(\x, 1, 2),element(\x, 1, 1)]</expression>
    <argument type="vector" index="1">
      <title>vector</title>
      <condition>element(\x, 1, 0) != 0</condition>
    </argument>
  </function>
  <function>
    <names>p2v</names>
    <expression>\x * [cos(\y), sin(\y)]</expression>
    <argument type="number" index="1">
      <title>r</title>
    </argument>
    <argument type="angle" index="2">
      <title>angle</title>
    </argument>
  </function>
</QALCULATE>
